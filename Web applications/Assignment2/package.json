    "start-test": "cross-env DBNAME=test.db node backend/dist/backend/src/ts/app.js",
    "test": "cross-env DBNAME=test.db mocha --timeout 10000 -r ts-node/register tests/all.test.ts --lib ES2015",
    "posttest": "rm -f test.db"
